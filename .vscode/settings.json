{
  "javascript.validate.enable": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "cSpell.userWords": ["customizability", "mapique", "tagspaces", "treeviz"],
  "typescript.format.enable": true,
  "search.exclude": {
    ".git": true,
    "cordova": true,
    "dll": true,
    "app/dist": true,
    "web": true,
    "node_modules": false
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "cSpell.words": [
    "Affero",
    "flowtype",
    "haftungsbeschraenkt",
    "lastindex",
    "nbsp",
    "taggroup",
    "taglibrary",
    "textcolor"
  ],
  "flow.enabled": false
}
