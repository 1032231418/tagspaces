{
  "name": "TagSpaces",
  "version": "1.1.1110",
  "manifest_version": 2,
  "description": "TagSpaces Explorer",
  "icons": {
    "16" : "icons/icon16.png",
    "48" : "icons/icon48.png",
    "64" : "icons/icon64.png",
    "128": "icons/icon128.png"    
  },
  "background": {
	"scripts": ["chrome/background.js"]
  },
  "options_page": "index.html",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },  
  "plugins": [
	{"path": "chrome/npapi-fileio/npapi-file-io-32.dll", "public": false},
	{"path": "chrome/npapi-fileio/npapi-file-io-32.so", "public": false},
	{"path": "chrome/npapi-fileio/npapi-file-io-64.so", "public": false}
  ],
  "permissions": [
    "tabs", "http://*/*"
  ]
}
